# Existing Database (DO NOT MODIFY)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=existing_database_name
POSTGRES_USER=existing_user
POSTGRES_PASSWORD=existing_password

# AbuseIPDB Enrichment
ABUSEIPDB_API_KEY=your_api_key
ABUSEIPDB_CONFIDENCE_MINIMUM=75
ABUSEIPDB_DAILY_LIMIT=10

# IOC Scoring Weights (Local detections prioritized as primary threat source)
LOCAL_CONFIDENCE_WEIGHT=0.8     # 80% weight for local detections (your attack reality)
EXTERNAL_CONFIDENCE_WEIGHT=0.2   # 20% weight for external sources (global context)
LOCAL_CONFIDENCE_BOOST=10

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_SECRET_KEY=your_secret_key
API_RATE_LIMIT=100

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/abuseipdb_ioc.log

# Redis Cache (optional but recommended for performance)
REDIS_URL=redis://localhost:6379/0
CACHE_TTL=3600

# Geolocation Services (dynamic rate limiting)
GEO_REQUEST_DELAY=1.0          # Base delay between geolocation requests (seconds)
GEO_MAX_DELAY=30.0             # Maximum delay for rate limiting backoff
GEO_BATCH_SIZE=100             # Batch size for processing IOCs

# TAXII 2.1 Server Configuration
TAXII_DEFAULT_LIMIT=100        # Default number of objects per TAXII response
TAXII_MAX_LIMIT=10000          # Maximum objects allowed per request

# Processing Configuration
PREPROCESSING_TTL=86400        # IOC preprocessing cache TTL (24 hours)
CORRELATION_BATCH_SIZE=100     # Batch size for IOC correlation processing
